{
  "name": "react-seat-toolkit",
  "version": "0.0.0",
  "description": "React UI library to design and render seat layouts",
  "main": "src/index.jsx",
  "private": false,
  "exports": {
    ".": "./src/index.jsx",
    "./components": "./src/components/index.jsx",
    "./constants": "./src/constants/index.js",
    "./icons": "./src/icons/index.js",
    "./hooks": "./src/hooks/index.js",
    "./providers": "./src/providers/index.js",
    "./styles": "./src/styles/index.css"
  },
  "scripts": {
    "build": "bun run ./esbuild.config.js && bun build:css",
    "build:css": "bunx tailwindcss -o ./dist/index.css --minify",
    "bump-version": "bun run --bun automatic-versioning --no-commit --recursive --prerelease-tag=blizzard --prerelease-branch=development -name=react-seat-toolkit",
    "format": "bunx prettier --write --cache \"**/*.{js,jsx,ts,tsx,md,css,yml}\"",
    "lint": "bun run --bun eslint . --ext js,jsx,ts,tsx,mdx --ignore-path .gitignore --fix --cache --report-unused-disable-directives",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "lefthook install",
    "release": "npm publish --access=public --no-git-checks || true",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sliit-foss/bashaway-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sliit-foss/bashaway-ui/issues"
  },
  "homepage": "https://github.com/sliit-foss/bashaway-ui#readme",
  "dependencies": {
    "@radix-ui/react-accordion": "1.1.2",
    "@radix-ui/react-toast": "1.1.4",
    "@radix-ui/react-tooltip": "1.0.6",
    "@reduxjs/toolkit": "1.9.5",
    "class-variance-authority": "0.7.0",
    "d3": "7.8.5",
    "framer-motion": "10.14.0",
    "lodash": "4.17.21",
    "lucide-react": "0.316.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-icons": "4.10.1",
    "react-loader-spinner": "5.3.4",
    "react-redux": "8.1.2",
    "react-router-dom": "6.14.0",
    "tailwind-merge": "1.13.2",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@sliit-foss/automatic-versioning": "2.1.1",
    "@storybook/addon-essentials": "7.3.2",
    "@storybook/addon-interactions": "7.3.2",
    "@storybook/addon-links": "7.3.2",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.3.2",
    "@storybook/manager-api": "7.3.2",
    "@storybook/react": "7.3.2",
    "@storybook/react-vite": "7.3.2",
    "@storybook/testing-library": "0.2.0",
    "@storybook/theming": "7.3.2",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "autoprefixer": "10.4.14",
    "bun-types": "1.0.21",
    "esbuild": "0.19.12",
    "esbuild-plugin-glob": "2.2.2",
    "eslint": "8.49.0",
    "eslint-plugin-mdx": "2.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.13",
    "lefthook": "1.4.3",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "storybook": "7.3.2",
    "storybook-addon-react-router-v6": "1.0.2",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "1.0.6",
    "vite": "4.3.9"
  }
}